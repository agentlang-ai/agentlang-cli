{
    "name": "agentlangcli",
    "version": "0.0.10",
    "description": "A command-line interface tool for AgentLang",
    "main": "out/main.js",
    "type": "module",
    "bin": {
        "agent": "./bin/cli.js"
    },
    "files": [
        "out/",
        "bin/",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsc -b tsconfig.src.json && node esbuild.mjs",
        "start": "ts-node src/index.ts",
        "dev": "ts-node src/index.ts",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "agentlang",
        "cli",
        "agent",
        "programming-language",
        "api-generation",
        "openapi",
        "swagger",
        "documentation"
    ],
    "author": "AgentLang Team",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/agentlang/agentlang-cli.git"
    },
    "bugs": {
        "url": "https://github.com/agentlang/agentlang-cli/issues"
    },
    "homepage": "https://github.com/agentlang/agentlang-cli#readme",
    "engines": {
        "node": ">=18.0.0"
    },
    "dependencies": {
        "@asteasolutions/zod-to-openapi": "^8.0.0-beta.4",
        "@redocly/cli": "^1.34.4",
        "agentlang": "^0.0.10",
        "chalk": "^5.4.1",
        "commander": "^14.0.0",
        "esbuild": "^0.25.5",
        "langium": "^3.5.0",
        "openapi-to-postmanv2": "^5.0.0",
        "zod": "^3.25.67"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/openapi-to-postmanv2": "^5.0.0",
        "typescript": "^5.0.0",
        "ts-node": "^10.9.0"
    }
}
