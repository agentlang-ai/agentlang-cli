{
  "name": "agentlangcli",
  "version": "0.0.62",
  "description": "A command-line interface tool for AgentLang",
  "main": "out/main.js",
  "type": "module",
  "bin": {
    "agent": "./bin/cli.js"
  },
  "files": [
    "out/",
    "bin/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.src.json && node esbuild.mjs",
    "start": "ts-node src/index.ts",
    "dev": "ts-node src/index.ts",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "check": "npm run typecheck && npm run lint:check && npm run format:check"
  },
  "keywords": [
    "agentlang",
    "cli",
    "agent",
    "programming-language",
    "api-generation",
    "openapi",
    "swagger",
    "documentation"
  ],
  "author": "AgentLang Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentlang/agentlang-cli.git"
  },
  "bugs": {
    "url": "https://github.com/agentlang/agentlang-cli/issues"
  },
  "homepage": "https://github.com/agentlang/agentlang-cli#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "7.3.4",
    "@redocly/cli": "^2.0.2",
    "agentlang": "^0.0.62",
    "chalk": "^5.4.1",
    "chokidar": "^4.0.3",
    "commander": "^14.0.0",
    "dotenv": "^17.2.1",
    "esbuild": "^0.25.8",
    "fs-extra": "^11.3.0",
    "langium": "^4.0.0",
    "openapi-client-axios": "^7.7.0",
    "openapi-to-postmanv2": "^5.0.1",
    "tmp": "^0.2.3",
    "yaml": "^2.8.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.1.0",
    "@types/openapi-to-postmanv2": "^5.0.0",
    "@types/tmp": "^0.2.6",
    "eslint": "^9.32.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.38.0"
  }
}
